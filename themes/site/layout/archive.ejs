<!-- Archive Header -->
<header>
	<h1>Update & Project Archives</h1>
</header>
<hr>

<!-- Posts -->
<section>
	<h2> All Posts </h2>
	<% for(let i = site.posts.data.length - 1; i >= 0; i--){%>
		<li>
			<a href="<%= url_for(site.posts.data[i].path) %>">
				<%- site.posts.data[i].title %>
			</a>
		</li>
	<% } %>
</section>

<!-- Pages -->
<section>
	<h2> All Pages </h2>
	<% for(let i = site.pages.data.length - 1; i >= 0; i--){%>
		<li>
			<a href="<%= url_for(site.pages.data[i].path) %>">
				<%- site.pages.data[i].title %>
			</a>
		</li>
	<% } %>
</section>